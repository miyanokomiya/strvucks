{
  "rules": {
    "env": {
      ".read": false,
      ".write": false
    },
    "webhooks": {
      ".read": false,
      ".write": false
    },
    "activities": {
      "$userId": {
        ".read": "auth.uid == $userId",
        ".write": "auth.uid == $userId"
      }
    },
    "users": {
      ".read": true,
      ".write": true
    },
    "ifttt": {
      "$userId": {
        ".read": "auth.uid == $userId",
        ".write": "auth.uid == $userId"
      }
    },
    "summaries": {
      "$userId": {
        ".read": "auth.uid == $userId",
        ".write": "auth.uid == $userId"
      }
    }
  }
}
